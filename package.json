    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.6",
    "@sentry/react": "^7.4.1",
    "@sentry/tracing": "^7.4.1",
    "axios": "^0.27.2",
    "clsx": "^1.2.0",
    "dayjs": "^1.11.3",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "notistack": "^2.0.5",
    "react": "17.x",
    "react-dom": "17.x",
