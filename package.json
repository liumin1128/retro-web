    "@sentry/react": "^7.11.1",
    "@sentry/tracing": "^7.11.1",
    "axios": "^0.27.2",
    "dayjs": "^1.11.5",
    "graphql": "^16.6.0",
    "graphql-ws": "^5.10.1",
    "lodash": "^4.17.21",
