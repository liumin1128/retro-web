    "@graphql-codegen/typescript-react-apollo": "^3.2.16",
    "@types/lodash": "^4.14.182",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/store": "^2.0.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
